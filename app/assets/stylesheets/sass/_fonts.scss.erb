@font-face {
    font-family: 'franchiseregular';
    src: url(<%= asset_path 'fonts/franchise-bold-webfont.eot' %>);
    src: url(<%= asset_path 'fonts/franchise-bold-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'fonts/franchise-bold-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'fonts/franchise-bold-webfont.ttf' %>) format('truetype'),
         url(<%= asset_path 'fonts/franchise-bold-webfont.svg#franchiseregular' %>) format('svg');
    font-weight: normal;
    font-style: normal;

}